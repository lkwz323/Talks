<script type="text/javascript">
    function myConfirm(msg, callback) {
        var d = $('#myModalDialog');
        d.find(".message").html(msg);
        d.find("a.btn-primary").click(callback);
        d.modal('show');
    }

    function myAlert(msg, title) {
        var d = $('#myModalAlert');
        d.find(".message").html(msg);
        title = title ? title : "提示";
        d.find(".modal-header h3").html(title);
        d.modal('show');
    }

</script>
