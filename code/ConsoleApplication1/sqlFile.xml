<?xml version="1.0" encoding="utf-8" ?>
<sqlMap namespace="Ibatis" xmlns="http://ibatis.apache.org/mapping" xmlns:xls="http://www.w3.org/2001/XMLSchema-instance">

  <statements>
    <statement id="FindPageId" parameterClass="System.String" resultClass="System.Guid" >
      SELECT Top(1) PageId
      FROM tpzPages
      WHERE FriendlyName =  #value#
    </statement>
  </statements>
</sqlMap>